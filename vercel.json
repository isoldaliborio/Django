{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      },
      {
        "src": "smartnote/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "smartnote/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "smartnote.settings",
      "PYTHONUNBUFFERED": "1"
    },
    "buildCommand": "pip install -r requirements.txt && python manage.py migrate && python manage.py collectstatic --noinput",
    "pythonVersion": "3.9"
  }
  